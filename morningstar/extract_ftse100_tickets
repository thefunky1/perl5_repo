		<script type="text/javascript">
			jQuery(document).ready(function() {
				var firstVoice = jQuery(".mega");
				var mainMenu = jQuery('#main-nav-container');
			
				<!-- Open megaMenu on click -->
				firstVoice.not('.menu-home').on('touchstart click', function (e) {
					if	(jQuery(this).hasClass("menu-selected")) {
					
					}else{
						firstVoice.not(this).removeClass("menu-selected");
						jQuery(this).addClass("menu-selected");
						e.preventDefault();
					}	
				});
				
				<!-- Close megaMenu -->
				function closeMenu(){
					if (firstVoice.hasClass('menu-selected')){
						firstVoice.removeClass('menu-selected');
					}
				}
				
				jQuery('html').bind({
					click: closeMenu,
					touchstart: function() {
						jQuery('html').unbind('click', closeMenu);
						closeMenu();
					}
				});
				
				mainMenu.on('touchstart click',function(event){
					event.stopPropagation();
				});
				
	 		});
		</script>	
	
