<script>

  function submitOnEnter(e,formId)
 {
	if( e.keyCode == '13' ){

	  clearInitial(formId);
	
	}
 }
 
 function submitOnEnterGeneric(e,formId)
 {
	if( e.keyCode == '13' ){

	  doSubmit(formId);
	
	}
 }
 

 function setInitial(initial,formId) {
   var form = document.getElementById(formId);
   if(formId === 'ismSearch') {
   	form.namecode.value='';
   } else {
   	form.nameCode.value='';
   }
   form.initial.value=initial;
   form.submit();
 }
 
 function clearInitial(formId) {
   var form = document.getElementById(formId);
   form.initial.value='';
   form.submit();
 }
 
 function doSubmit(formId) {
   var form = document.getElementById(formId);
   form.submit();
 }
 
 
 function deleteBlanks(str) {
		var strResult=str.replace(/ /g,"");
		strResult=strResult.replace(/&/g ,"");
		return strResult;
	}

 
</script>





