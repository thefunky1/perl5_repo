	<script type="text/javascript">
	function cleanQ() {
		var qElement = jQuery('#head_solr_search_input');
		if(qElement.prop('defaultValue') == qElement.val()) {
			qElement.val('');
		}	
	}
	jQuery(document).ready(function() {
		var msgLabels = {
			quotes : 'Quotations', 
			news : 'News',
			pages : 'Pages and Documents',
			showAll : 'Show all',
			noResult : 'No results found'
		};		      var searchUrl = {
				quotes : '/exchange/searchengine/quotes/search.html',
				news : '/exchange/searchengine/news/search.html',
				pages : '/exchange/searchengine/pages/search.html',
				json : '/exchange/searchengine/all/json/search.html'      
			  };
				enableAutocomplete('#head_solr_search_input', 'div.head_solr_search', msgLabels, searchUrl);
	});
	</script>
						
